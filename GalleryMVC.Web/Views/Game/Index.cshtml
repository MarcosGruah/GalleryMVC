@model List<Game>

<h1>Game List</h1>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>Game Id</th>
        <th>Game Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
        <tr>
            <td>@obj.GameId</td>
            <td>@obj.Name</td>
        </tr>
        }
    </tbody>
</table>